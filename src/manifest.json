{
	"name": "Blanka",
	"version": "0.1.0",
	"manifest_version": 2,
	"description": "A useful little Chrome Extension for removing target='_blank' globally.",
	"homepage_url": "http://hankchizljaw.io",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["blanka.js"]
		}
	],
	"permissions": [
		"https://*/*",
		"http://*/*",
		"tabs"
	]
}